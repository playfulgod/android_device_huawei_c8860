# mount point	fstype    	device    	[device2]

/boot		vfat	/dev/block/mmcblk0p1
/recovery      	vfat	/dev/block/mmcblk0p1
/cache		ext4    /dev/block/mmcblk0p6						
/system		ext4	/dev/block/mmcblk0p12   					
/data		ext4	/dev/block/mmcblk0p13						
/sdcard		vfat	/dev/block/mmcblk1p1    /dev/block/mmcblk1k1
/emmc           vfat    /dev/block/mmcblk0p14	/dev/block/mmcblk0
/sd-ext		ext4	/dev/block/mmcblk1p2

